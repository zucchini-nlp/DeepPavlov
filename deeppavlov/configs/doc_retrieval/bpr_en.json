{
  "chainer": {
    "in": ["questions"],
    "out": ["par_text"],
    "pipe": [
      {
        "class_name": "bpr",
        "load_path": "{MODEL_PATH}",
        "bpr_checkpoint": "bpr_finetuned_nq_adv.ckpt",
        "bpr_index": "bpr_finetuned_nq_adv.idx",
        "top_n": 10,
        "in": ["questions"],
        "out": ["par_ids"]
      },
      {
        "class_name": "wiki_sqlite_vocab",
        "in": ["par_ids"],
        "out": ["par_text"],
        "join_docs": false,
        "shuffle": false,
        "load_path": "{DOWNLOADS_PATH}/bpr/eng/enwiki_l100.db"
      }
    ]
  },
  "metadata": {
    "variables": {
      "ROOT_PATH": "~/.deeppavlov",
      "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
      "MODELS_PATH": "{ROOT_PATH}/models",
      "MODEL_PATH": "{MODELS_PATH}/bpr/eng"
    },
    "download": [
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/bpr/eng/bpr_eng.tar.gz",
        "subdir": "{MODEL_PATH}"
      },
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/bpr/eng/bpr_db_eng_l100.tar.gz",
        "subdir": "{DOWNLOADS_PATH}/bpr/eng"
      }
    ]
  }
}
